<%= render 'users/info', user: @user %>
<div class="col-md-8 offset-md-1">
  <h2>Users</h2>
    <table class="table table-striped">
      <tr>
        <td>image</td>
        <td>name</td>
        <td></td>
      </tr>
      <% @users.each do |user| %>
        <tr>
          <td><%= attachment_image_tag @user, :profile_image, format: 'jpeg', fallback: "no_image.jpg", size: "30x30" %></td>
          <td><%= user.name %></td>
          <td><%= link_to "Show", user_path(user.id) %></td>
        </tr>
      <% end %>
    </table>
</div>